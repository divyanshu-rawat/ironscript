{
  "name": "ironscript",
  "version": "1.0.3",
  "description": "A Minimal LISP-like Abstraction",
  "main": "./dist/node/ironscript.js",
  "scripts": {
    "test": "./dist/node/ironscript.js ./test/test.is",
    "build": "npm run build-node && npm run build-browser",
    "build-node": "rollup -c ./rollup.node.config.js && echo \"#!/usr/bin/env node \n \" | cat - ./dist/node/ironscript.js > ./dist/node/tmpfile && mv ./dist/node/tmpfile ./dist/node/ironscript.js && chmod +x ./dist/node/ironscript.js",
    "build-browser": "rollup -c rollup.browser.config.js"
  },
  "bin": {
    "ironscript": "./dist/node/ironscript.js"
  },
  "keywords": [
    "meta",
    "language",
    "lisp",
    "async",
    "stream",
    "rewriting",
    "scheme",
    "functional"
  ],
  "author": "Ganesh Prasad Sahoo <sir.gnsp@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "async-es": "^2.1.2"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.2.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-resolve": "^2.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GnsP/ironscript.git"
  },
  "bugs": {
    "url": "https://github.com/GnsP/ironscript/issues"
  },
  "homepage": "https://github.com/GnsP/ironscript#readme"
}
